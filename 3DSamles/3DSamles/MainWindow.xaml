<Window x:Class="StereoCanvasSamples.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StereoCanvasSamples"
        mc:Ignorable="d"
        Title="MainWindow" Height="504" Width="442">
    <Grid Height="450" Margin="10,0,10,0">
        <Button x:Name="buttonCube" Content="Куб" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" FontSize="14" Width="402" Click="buttonCube_Click"/>
        <Button x:Name="buttonMultipointSurfaces" Content="Поверхности безье" HorizontalAlignment="Center" Margin="0,38,0,0" VerticalAlignment="Top" FontSize="14" Width="402" Click="buttonBezierSurfaces_Click"/>
        <Canvas x:Name="PreviewCanvas" HorizontalAlignment="Center" Margin="0,95,0,0" Width="52" Height="52" VerticalAlignment="Top"/>
    </Grid>
</Window>
